        <TLCOrganization eId="os" href="/ontology/organization/OsnovniSud.BijeloPolje" showAs="Osnovni sud u Bijelom Polju"/>
        <TLCPerson eId="ajlovric" href="/ontology/person/ajlovric" showAs="Anja Lovric"/>
        <TLCPerson eId="aduic" href="/ontology/person/aduic" showAs="Anja Ducic"/>
        <TLCPerson eId="dvukovic" href="/ontology/person/dvukovic" showAs="Dusan Vukovic"/>
        <TLCPerson eId="ttatjanic" href="/ontology/person/ttatjanic" showAs="Tatjana Tatjanic"/>
        <TLCPerson eId="pperic" href="/ontology/person/pperic" showAs="Pera Peric"/>
